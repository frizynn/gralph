# Example tasks.yaml for GRALPH
# Copy this file to tasks.yaml and customize for your project

version: 1
branchName: gralph/example-feature
tasks:
  - id: TASK-001
    title: Add users table migration
    completed: false
    dependsOn: []
    touches: ["db/migrations/**"]

  - id: TASK-002
    title: Create user service and endpoints
    completed: false
    dependsOn: ["TASK-001"]
    touches: ["src/**"]

  - id: TASK-003
    title: Build user profile UI
    completed: false
    dependsOn: ["TASK-002"]
    touches: ["src/**"]
    verify: ["npm test"]
